{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AuthConfig": {
    "IdPUrl": "https://localhost:5001",
    "ClientId": "cmsclient",
